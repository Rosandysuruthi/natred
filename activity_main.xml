<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.suruthi.naturalremedies.MainActivity">

    <ImageView
        app:srcCompat="@drawable/naturalhealth"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/imageView3"
        android:cropToPadding="false"
        android:layout_height="84dp"
        android:layout_width="100dp"
        tools:ignore="ContentDescription" />

    <Button
        android:text="HEALTH"
        android:layout_height="84dp"
        android:id="@+id/button"
        android:layout_width="204dp"
        android:textStyle="normal|bold"
        android:textAlignment="center"
        android:fontFamily="sans-serif-medium"
        android:textSize="22sp"
        android:onClick="h1"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="11dp"
        android:layout_marginEnd="11dp"
        tools:ignore="HardcodedText,UnusedAttribute" />

    <ImageView
        app:srcCompat="@drawable/dailyhygiene"
        android:id="@+id/imageView5"
        android:cropToPadding="false"
        android:layout_height="84dp"
        android:layout_width="100dp"
        android:layout_marginLeft="13dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="82dp"
        android:layout_below="@+id/button1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        tools:ignore="ContentDescription" />

    <Button
        android:text="DAILY HYGIENE"
        android:layout_height="84dp"
        android:id="@+id/button2"
        android:layout_width="204dp"
        android:textStyle="normal|bold"
        android:textAlignment="center"
        android:fontFamily="sans-serif-medium"
        android:textSize="22sp"
        android:layout_alignTop="@+id/imageView5"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        tools:ignore="HardcodedText,UnusedAttribute"

        android:elevation="0dp" />

    <Button
        android:text="NATURAL BEAUTY"
        android:layout_height="84dp"
        android:id="@+id/button1"
        android:layout_width="204dp"
        android:textStyle="normal|bold"
        android:textAlignment="center"
        android:fontFamily="sans-serif-medium"
        android:textSize="22sp"
        android:onClick="Ny"
        tools:ignore="HardcodedText,UnusedAttribute"
        android:layout_alignBottom="@+id/imageView4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="13dp" />

    <ImageView
        app:srcCompat="@drawable/naturalbeauty"
        android:id="@+id/imageView4"
        android:cropToPadding="false"
        android:layout_height="84dp"
        android:layout_width="100dp"
        android:layout_marginTop="92dp"
        tools:ignore="ContentDescription"
        android:layout_below="@+id/button"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />


</RelativeLayout>
